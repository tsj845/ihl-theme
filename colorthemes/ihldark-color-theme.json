{
    "semanticHighlighting" : true,
    "tokenColors" : [
        {
            "scope" : "halter.ihl",
            "settings" : {
                "foreground" : "#c83232"
            }
        },
        {
            "scope" : ["meta.tag.ihl"],
            "settings" : {
                "foreground" : "#45afff"
            }
        },
        {
            "scope" : ["source", "meta.tag.escaped.ihl"],
            "settings" : {
                "foreground" : "#d4d4d4"
            }
        },
        {
            "scope" : ["string"],
            "settings" : {
                "foreground" : "#d89845"
            }
        },
        {
            "scope" : ["entity.name.type.class", "entity.name.type", "constant.other.ihl"],
            "settings" : {
                "foreground" : "#36c7cc"
            }
        },
        {
            "scope" : ["storage.type", "variable.language", "constant", "keyword", "operator.math.ihl", "dollar.ihl", "storage.modifier.mut.rust", "storage.modifier.rust"],
            "settings" : {
                "foreground" : "#569cd6"
            }
        },
        {
            "scope" : "constant.numeric",
            "settings" : {
                "foreground" : "#b5cea8"
            }
        },
        {
            "scope" : "comment",
            "settings" : {
                "foreground" : "#23a323"
            }
        },
        {
            "scope" : "constant.language.json.comments",
            "settings" : {
                "foreground" : "#19dcff"
            }
        },
        {
            "scope" : "keyword.debug.ihl",
            "settings" : {
                "foreground" : "#d68fd6"
            }
        },
        {
            "scope" : "punctuation.definition.lifetime.rust",
            "settings" : {
                "foreground" : "#ffcb3d"
            }
        },
        {
            "scope" : "variable.other.rust",
            "settings" : {
                "foreground" : "#a08bec"
            }
        },
        {
            "scope" : ["entity.name.function.rust", "meta.macro.rust"],
            "settings" : {
                "foreground" : "#6db5ca"
            }
        },
        {
            "scope" : "constant.other.caps.rust",
            "settings" : {
                "foreground" : "#c685d3"
                // "foreground" : "#ee89b8"
            }
        },
        {
            "scope" : "error.ihl",
            "settings" : {
                "foreground" : "#c83232"
            }
        }
    ]
}